{
  "name": "bootstrap4-webpack2-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:mihaerzen/bootstrap4-webpack2-boilerplate.git",
  "author": "Miha Er≈æen <miha.erzen1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "npm run eslint",
    "eslint": "./node_modules/.bin/eslint .",
    "rmrf": "rm -rf dist",
    "gh-pages": "node build/gh-pages.js",
    "build:dev": "npm run rmrf && webpack --env.development",
    "build:prod": "npm run rmrf && webpack -p --env.production",
    "build": "npm run build:dev",
    "serve": "webpack-dev-server --env.development --content-base dist --open --inline --hot --host 0.0.0.0",
    "serve:prod": "webpack-dev-server --env.production --content-base dist --host 0.0.0.0"
  },
  "dependencies": {
    "args": "^3.0.8",
    "bootstrap": "4.0.0",
    "child_process": "^1.0.2",
    "fs": "^0.0.1-security",
    "holderjs": "^2.9.4",
    "jquery": "^3.2.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "path": "^0.12.7",
    "popper.js": "^1.12.9",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.29.0",
    "imagemin-webpack-plugin": "^1.5.0-beta.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
